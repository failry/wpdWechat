/**index.wxss**/

page {
  height: 100%;
  background: #f2f2f2;
}

.page-main {
  width: 100%;
}

.J-swiper {
  width: 100%;
  height: 400rpx;
}

.slide-image {
  display: block;
  width: 100%;
  height: 400rpx;
}

.page-news {
  color: #666;
  font-size: 24rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #e0e0e0;
  background: #fff;
}

.icon-news {
  display: inline-block;
  width: 78rpx;
  height: 70rpx;
}

.swiper_container {
  width: 80vw;
  height: 70rpx;
  line-height: 70rpx;
}

.swiper_item {
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  letter-spacing: 2px;
}

.span-news {
  position: relative;
  top: 2rpx;
}

.page-tab {
  background: #fff;
  padding: 25rpx 0;
}

.page-tab-image {
  width: 110rpx;
  height: 110rpx;
}

.page-tab-text {
  color: #333;
  font-size: 24rpx;
  font-weight: bold;
}

.page-recommend {
  background: url('data:image/png;base64,');
  width: 100%;
  height: 482rpx;
  background-size: 100%;
  margin-top: 20rpx;
  position: relative;
}

.recommend-title {
  position: absolute;
  color: #fff;
  font-size: 24rpx;
  right: 30rpx;
  top: 12rpx;
  width: 160rpx;
  height: 30rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.recommend-time {
  width: 130rpx;
  height: 130rpx;
  position: absolute;
  top: 240rpx;
  left: 50rpx;
}

.recommend-qx {
  font-size: 24rpx;
  color: #808080;
  display: block;
}

.recommend-span {
  margin-top: 10rpx;
  color: #333;
  display: block;
  font-weight: bold;
  font-size: 32rpx;
}

.recommend-em {
  font-size: 24rpx;
  padding-left: 4rpx;
  position: relative;
  top: -2rpx;
}

.recommend-sum {
  width: 170rpx;
  height: 130rpx;
  position: absolute;
  top: 240rpx;
  right: 10rpx;
}

.page-bscg-text {
  width: 100%;
  text-align: center;
  color: #686b6d;
  font-size: 22rpx;
  height: 70rpx;
  background: #f2f2f2;
  display: flex;
  align-items: center;
  justify-content: center;
}

.page-bscg-image {
  width: 60rpx;
  height: 70rpx;
  position: relative;
  top: -1px;
}

.circleProgress_wrapper {
  width: 300rpx;
  height: 300rpx;
  position: absolute;
  top: 60rpx;
  left: 50%;
  margin-left: -148rpx;
  /* background: rgba(0,0,0,.4)      */
}
 canvas {
  width: 100%;
  height: 100%;
} 
.circleprogress {
  width: 252rpx;
  height: 252rpx;
  border: 15rpx solid #ffaa00;
  border-radius: 100%;
  border-left: 15rpx solid transparent;
  border-top: 15rpx solid transparent;
  transform: rotate(-135deg);
}

.recommend-nhlv {
  width: 250rpx;
  height: 80rpx;
  color: #fff;
  position: absolute;
  top: 180rpx;
  left: 50%;
  margin-left: -125rpx;
  text-align: center;
  font-size: 28rpx;
}

.nhlv-span1 {
  font-size: 54rpx;
}

.nhlv-span2 {
  font-size: 42rpx;
}
